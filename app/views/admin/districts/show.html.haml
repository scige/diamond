%h2 Show district

= form_for [:admin, @district], :html => {:class=>"form-horizontal"} do |f|
  .control-group
    = f.label :name, :class=>"control-label"
    .controls
      = f.text_field :name, :class=>"input-xxlarge", :disabled=>"disabled"

  .control-group
    .controls
      = link_to 'Edit', edit_admin_district_path(@district), :class=>"btn btn-info"
      = link_to 'Back', admin_districts_path, :class=>"btn"

